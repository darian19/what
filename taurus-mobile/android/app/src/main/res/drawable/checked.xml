<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="5dp" android:left="5dp" android:right="5dp" android:bottom="5dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="rectangle">
            <stroke android:width="1dp" android:color="#FFFFFF"/>
            <size android:height="15dp" android:width="15dp"/>
        </shape>
    </item>
    <item android:top="2dp" android:left="8dp" android:right="0dp" android:bottom="5dp">
        <rotate android:fromDegrees="-45" android:toDegrees="45">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="line">
            <stroke android:width="3dp" android:color="#00B0F0"/>
        </shape>
        </rotate>
    </item>
    <item android:top="6dp" android:left="6dp" android:right="9dp" android:bottom="4dp">
        <rotate android:fromDegrees="45" android:toDegrees="45">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                   android:shape="line">
                <stroke android:width="3dp" android:color="#00B0F0"/>
            </shape>
        </rotate>
    </item>
</layer-list>
