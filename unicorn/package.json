    "node-sass": "3.2.0",
    "react": "0.13.3",
    "react-tap-event-plugin": "0.1.7"
  },
  "devDependencies": {
    "babel": "5.8.19",
    "babel-core": "5.8.19",
    "babel-loader": "5.3.2",
    "babel-plugin-typecheck": "1.2.0",
    "casperjs": "1.1.0-beta3",
    "electron-prebuilt": "0.30.2",
    "gulp": "3.9.0",
    "gulp-util": "3.0.6",
