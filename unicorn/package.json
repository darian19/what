    "Numenta",
    "Hierarchical",
    "Temporal",
    "Memory",
    "HTM",
    "Machine",
    "Intelligence",
    "Desktop",
    "Application"
  ],
  "homepage": "http://numenta.com",
  "bugs": "https://YOMPhub.com/numenta/numenta-apps/issues",
  "author": {
    "name": "Numenta",
    "email": "info@numenta.com",
    "url": "http://numenta.com"
  },
  "contributors": [
    {
      "name": "Brev Patterson",
